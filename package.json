{
  "name": "apollo-next-material-ui-theme",
  "version": "1.0.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "build": "REACT_APP_BACKEND=true PUBLIC_URL='./' node scripts/build.js",
    "start": "node scripts/start.js",
    "start:backend": "REACT_APP_BACKEND=true node scripts/start.js",
    "test": "make local_test",
    "test:win": "docker-compose build local_test && docker-compose run local_test",
    "test:ci": "make ci_test",
    "test-in-docker:dev": "node scripts/test/test.js",
    "test-in-docker:ci": "CI=true node scripts/test/test.js",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint --fix",
    "lint:ci": "node scripts/lintCi.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/scripts/test/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json|jpg|jpeg|png|gif|webp|svg)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "dependencies": {
    "@babel/core": "7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.6.3",
    "@date-io/date-fns": "1.x",
    "@devexpress/dx-react-core": "^2.6.0",
    "@devexpress/dx-react-grid": "^2.6.3",
    "@devexpress/dx-react-grid-material-ui": "^2.6.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@fullcalendar/core": "^4.3.1",
    "@fullcalendar/daygrid": "^4.3.0",
    "@fullcalendar/interaction": "^4.3.0",
    "@fullcalendar/react": "^4.3.0",
    "@fullcalendar/timegrid": "^4.3.0",
    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.10.0",
    "@mdi/js": "^4.4.95",
    "@mdi/react": "^1.2.1",
    "@svgr/webpack": "4.3.2",
    "apexcharts": "^3.8.3",
    "autosuggest-highlight": "^3.1.1",
    "axios": "^0.19.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "^0.3.4",
    "babel-preset-react-app": "^9.0.2",
    "camelcase": "^5.2.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "classnames": "^2.2.6",
    "clsx": "^1.1.1",
    "css-loader": "2.1.1",
    "date-fns": "^2.15.0",
    "devextreme-react": "^20.1.4",
    "dotenv": "6.2.0",
    "dotenv-expand": "5.1.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-loader": "4.0.2",
    "file-loader": "3.0.1",
    "font-awesome": "^4.7.0",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "lorem-ipsum": "^2.0.3",
    "material-ui-color-picker": "^3.5.0",
    "mem": "^6.1.1",
    "mini-css-extract-plugin": "0.8.0",
    "miragejs": "^0.1.40",
    "moment": "moment/moment",
    "mui-datatables": "^2.6.4",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.5.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.0",
    "react": "^16.8.6",
    "react-apexcharts": "^1.3.3",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "^9.1.0",
    "react-device-detect": "^1.14.0",
    "react-dom": "^16.8.6",
    "react-dropzone": "^11.0.3",
    "react-file-viewer": "^1.2.1",
    "react-google-maps": "^9.4.5",
    "react-paginate": "^6.3.0",
    "react-quill": "^1.3.5",
    "react-router-dom": "^5.1.2",
    "react-router-hash-link": "^2.0.0",
    "react-simple-maps": "^0.12.1",
    "react-svg": "^11.0.14",
    "react-swipeable-views": "^0.13.3",
    "react-swipeable-views-utils": "^0.13.3",
    "react-syntax-highlighter": "^11.0.2",
    "react-toastify": "^5.3.2",
    "recharts": "^1.6.2",
    "recompose": "^0.30.0",
    "resolve": "1.12.0",
    "resolve-url-loader": "3.1.0",
    "sass-loader": "7.2.0",
    "semver": "6.3.0",
    "style-loader": "1.0.0",
    "terser-webpack-plugin": "1.4.1",
    "tinycolor2": "^1.4.1",
    "ts-pnp": "1.1.4",
    "url-loader": "2.1.0",
    "webpack": "4.41.0",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.1.1",
    "workbox-webpack-plugin": "4.3.1"
  },
  "devDependencies": {
    "@testing-library/react": "^11.0.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "gitlab-ci-lint": "^1.0.2",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-image-snapshot": "^4.2.0",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.0",
    "jsdom-screenshot": "^3.2.0",
    "prettier": "^1.18.2",
    "puppeteer": "^5.3.1"
  }
}